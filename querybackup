 insert into queries (query, query_name) values('select * from staff;','get_staff');
 insert into queries (query, query_name) values('select team_name, staff_id from teams, team_members where team_members.team_id = teams.team_id;', 'get_teams');
 insert into queries (query, query_name) values('insert into teams (team_name) values (?);', 'make_new_team');
 insert into queries (query, query_name) values('insert into staff (username, timezone, contact_methods, activity_level, level) values (?,?,?,?, ?) returning staff_id','insert_staff');
 insert into queries (query, query_name) values('update staff set username = ?, timezone = ?, contact_methods = ?, activity_level = ?, levels = ? where staff_id = ?','update_staff');
 insert into queries (query, query_name) values('insert into team_members (team_id, staff_id) values ((select team_id from teams where team_name = ?),?);', 'add_to_team');
 insert into queries (query, query_name) values('delete from staff where staff_id = ?;','remove_staff');
 insert into queries (query, query_name) values('delete from team_members where staff_id = ?;', 'remove_staff_from_teams');
